{
  "name": "yocto-queue2",
  "version": "2.0.0",
  "description": "Tiny queue data structure",
  "license": "MIT",
  "repository": "maanimis/yocto-queue2",
  "funding": "https://github.com/sponsors/maanimis",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "https://sindresorhus.com"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "engines": {
    "node": ">=12.20"
  },
  "scripts": {
    "clear": "rimraf dist",
    "build": "tsup",
    "compile": "npm run clear && tsc",
    "check": "biome check",
    "lint": "biome lint",
    "format": "biome format",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "keywords": [
    "queue",
    "data",
    "structure",
    "algorithm",
    "queues",
    "queuing",
    "list",
    "array",
    "linkedlist",
    "fifo",
    "enqueue",
    "dequeue",
    "data-structure"
  ],
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@biomejs/biome": "2.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "24.9.1",
    "babel-jest": "30.2.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.6",
    "rimraf": "6.0.1",
    "tsup": "8.5.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  }
